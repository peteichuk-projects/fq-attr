{
  "name": "Find QA attributes",
  "description": "Find QA attributes on page",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/popup.js"
      ],
      "matches": [
        "\u003Call_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/icon128.png"
  ],
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}
